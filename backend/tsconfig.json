{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "module": "commonjs",
     "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "esModuleInterop": true,
    "moduleResolution":"Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.js","**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules","build"]
 }